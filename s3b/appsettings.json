{
  "appsettings": {
    "db.connection": "Data Source=c:\\temp\\s3b\\data\\s3b.db",
    "s3b.temp": "c:\\temp\\s3b\\temp",

    "archive.enabled": 1,
    "archive.command": "tar.exe",
    "archive.args": "rvf $(temp)\\$(archivetarget).tar \"$(localobject)\"",
    "archive.clean": "$(temp)\\$(archivetarget).tar",

    "compress.enabled": 1,
    "compress.command": "gzip.exe",
    "compress.args": "-f \"$(temp)\\$(archivetarget).tar\"",
    "compress.clean": "$(temp)\\$(archivetarget).tar.gz",

    "encrypt.enabled": 1,
    "encrypt.command": "openssl.exe",
    "encrypt.args": "enc -aes-256-cbc -iter 3 -pass file:$(passfile) -out \"$(temp)\\$(archivetarget).tar.gz.enc\" -in \"$(temp)\\$(archivetarget).tar.gz\"",
    "encrypt.clean": "$(temp)\\$(archivetarget).gz.enc",

    "upload.enabled": 1,
    "upload.command": "aws.exe",
    "upload.args": "s3 cp \"$(temp)\\$(archivetarget).tar.gz.enc\" s3://$(uploadtarget)/$(archivetarget).tar.gz.enc",


    "decrypt.enabled": 1,
    "decrypt.command": "openssl.exe",
    "decrypt.args": "enc -d -aes-256-cbc -iter 3 -pass file:$(passfile) -in \"$(temp)\\$(archivetarget).gz.enc\" -out \"$(temp)\\$(archivetarget).gz\"",
    "decrypt.clean": "$(temp)\\$(archivetarget).gz.enc"
  }
}
